    "nanoid": "4.0.2",
    "next": "13.4.13-canary.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.2",
    "redaxios": "0.5.1",
    "sonner": "0.6.2",
    "swr": "2.2.0",
    "tailwind-merge": "1.14.0",
