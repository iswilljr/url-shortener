    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "supabase": "1.82.2",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "1.0.6",
    "typescript": "5.1.6"
