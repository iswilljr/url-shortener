  "devDependencies": {
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "encoding": "0.1.13",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.9.0",
    "eslint-config-standard-jsx": "11.0.0",
    "eslint-config-standard-with-typescript": "37.0.0",
