{
  "name": "xlug",
  "description": "A URL shortener built with NextJs",
  "version": "2.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "iswilljr <iswilljr@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "db:gen": "(supabase gen types typescript --local > src/types/supabase.d.ts) && eslint --fix src/types/supabase.d.ts",
    "db:lint": "supabase db lint",
    "db:reset": "supabase db reset || supabase start",
